{
  "parser": "babel-eslint",
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "env": {
    "browser": true,
    "jest": true
  },
  "plugins": [
    "import"
  ],
  "rules": {
    "quotes": [
      2,
      "single",
      "avoid-escape"
    ],
    "no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "^_"
      }
    ],
    "global-require": "off",
    "import/prefer-default-export": "off"
  }
}
